n":0},"source":"index.js","original":{"line":91,"column":4}},{"generated":{"line":120,"column":47},"source":"index.js","original":{"line":91,"column":57}},{"generated":{"line":121,"column":0},"source":"index.js","original":{"line":91,"column":57}},{"generated":{"line":122,"column":0},"source":"index.js","original":{"line":106,"column":5}},{"generated":{"line":123,"column":0},"source":"index.js","original":{"line":107,"column":1}},{"name":"manageResources","generated":{"line":125,"column":0},"source":"index.js","original":{"line":109,"column":0}},{"generated":{"line":125,"column":15},"source":"index.js","original":{"line":109,"column":15}},{"name":"Clickers","generated":{"line":125,"column":16},"source":"index.js","original":{"line":109,"column":16}},{"generated":{"line":125,"column":24},"source":"index.js","original":{"line":109,"column":15}},{"generated":{"line":125,"column":25},"source":"index.js","original":{"line":109,"column":0}}],"sources":{"index.js":"\nvar root = document.querySelector('#root');\n\nvar Resources = {\n    'Birds': 0,\n    'Birdseed': 100,\n    'Trinkets': 0\n};\n\nvar Clickers = {\n    'scatterBirdseed': {\n        'name': 'Scatter some birdseed',\n        'buy': Resources.Trinkets, // resource to buy\n        'buyCount': 1, // how many?\n        'spend': Resources.Birdseed, // resource to spend\n        'spendCount': 10, // how many?\n        'successMessage': 'A bird ate some birdseed and flew away, leaving you 1 trinket.',\n        'errorMessage': 'Not enough birdseed'\n    },\n    'buyBirdseed': {\n        'name': 'Buy more birdseed',\n        'buy': Resources.Birdseed,\n        'buyCount': 10,\n        'spend': Resources.Trinkets,\n        'spend.count': 1,\n        'successMessage': '',\n        'errorMessage': 'Not enough trinkets'\n    }\n};\n\n\n// component functions\nfunction Header(){\n    return `\n        <header>Bird Clicker: an Incremental Game</header>\n    `;\n}\n\nfunction Counters(resources){\n    var output = '<div id=\"resources\" class=\"column\">';\n\n    for(const [ key, value ] of Object.entries(resources)){\n        if(value > 0){\n            output += `<span id=\"${key}\">${key}: ${value}</span>`;\n        }\n    }\n\n    output += '</div>';\n\n    return output;\n}\n\nfunction Buttons(clickers){\n    var output = '<div id=\"clickers\" class=\"column\">';\n\n    // for each object in the Clickers array, create a button with a name that manipulates resources\n    for(const [ key, value ] of Object.entries(clickers)){\n        output += `<span id=\"${key}\" class=\"button\">${value.name}</span>`;\n    }\n    \n    output += '</div>';\n    \n    return output;\n}\n\nfunction Messages(){\n    return '<div id=\"messages\"></div>';\n}\n\n// rendering HTML content\n\nfunction render(){\n    root.innerHTML = `\n    ${Header()}\n    ${Counters(Resources)}\n    ${Buttons(Clickers)}\n    ${Messages()}\n`;\n}\nrender();\n\n// user alert message\nfunction newMessage(text){\n    var destination = document.querySelector('#messages');\n    \n    destination.innerHTML += `<span>${text}</span>`;\n}\n\n// making buttons do things\nfunction manageResources(clickers){\n    for(const [ key, value ] of Object.entries(clickers)){\n        let button = document.getElementById(`${key}`);\n\n        button.addEventListener('click', (event) => {\n            if(value.spend >= value.spendCount){\n                value.buy += value.buyCount;\n                value.spend -= value.spendCount;\n                // update counters???\n                newMessage(`${value.successMessage}`);\n            }\n            else{\n                newMessage(`${value.errorMessage}`);\n            }\n            console.log(Resources);\n        });\n    }\n}\n\nmanageResources(Clickers);"},"lineCount":null}},"hash":"469663888540b6c47ca5c12cb0c056d2","cacheData":{"env":{}}}